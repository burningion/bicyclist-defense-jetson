{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --host 0.0.0.0 --reload",
    "fastapi-resnet-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.wip_resnet:app --host 0.0.0.0 --reload",
    "next-dev": "next dev -H 0.0.0.0",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "resnet-dev": "concurrently \"npm run next-dev\" \"npm run fastapi-resnet-dev\"",
    "build": "next build",
    "start": "concurrently \"python3 -m uvicorn api.index:app --host 0.0.0.0\" \"next start\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0-alpha.4",
    "@heroicons/react": "^2.1.1",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "clsx": "^2.1.0",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "next": "14.1.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "5.0.4"
  }
}
